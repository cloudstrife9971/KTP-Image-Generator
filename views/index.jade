    script.
      function randomIntFromInterval(min, max) { // min and max included
      return Math.floor(Math.random() * (max - min + 1) + min)
      }
      var rndInt = randomIntFromInterval(1, 49)

      var singleUserData = allUserData[rndInt]
  
      var rndInt = randomIntFromInterval(1, 12)
      var singleUserSource = userImages[rndInt]

   
      document.getElementsByClassName("profileimg")[0].src = "/" + singleUserSource.source

      document.getElementsByClassName("nik")[0].value = (Math.random() * 100000000000000000).toString().substring(0,16)

      //- ----------------------------------------------------------------------------------------------------------

      if(document.getElementsByClassName("name")[0].value=="undefined"){
      document.getElementsByClassName("name")[0].value = singleUserData.name
      }
      //- document.getElementsByClassName("name")[0].value = singleUserData.name
        if(document.getElementsByClassName("region")[0].value=="undefined"){
          document.getElementsByClassName("region")[0].value = "KENT"
      }
      //- document.getElementsByClassName("region")[0].value = "KENT"
        if(document.getElementsByClassName("tem")[1].value =="undefined"){
        document.getElementsByClassName("tem")[1].value = singleUserData.date1
      }
      //-  document.getElementsByClassName("tem")[1].value = singleUserData.date1




      //- ---------------------------------------------------------------------------------------------------------------
        if(document.getElementsByClassName("jenis")[0].value =="undefined"){
        document.getElementsByClassName("jenis")[0].value = singleUserSource.gender == "male" ? "laki-laki" : "perempuan"
      }
    

       if(document.getElementsByClassName("golDarah")[0].value =="undefined"){
        document.getElementsByClassName("golDarah")[0].value = "O+"
      }
     

       if(document.getElementsByClassName("alamat")[0].value =="undefined"){
        document.getElementsByClassName("alamat")[0].value = singleUserData.address.substring(0,20)
      }
      

      //-  if(document.getElementsByClassName("rt")[0].value =="undefined"){
      //-   document.getElementsByClassName("rt")[0].value = "004/026"
      //- }
      //- document.getElementsByClassName("rt")[0].value = "004/026"

       if( document.getElementsByClassName("kel")[0].value =="undefined"){
       document.getElementsByClassName("kel")[0].value = singleUserData.region
      }
    

       if(document.getElementsByClassName("kec")[0].value =="undefined"){
        document.getElementsByClassName("kec")[0].value = singleUserData.city
      }
 

       if(document.getElementsByClassName("agama")[0].value =="undefined"){
        document.getElementsByClassName("agama")[0].value = ["islam", "kristen", "katholik", "hindu", "budha", "khonghucu", "penghayat kepercayaan", "lainnya"][Math.floor(Math.random() * 8)]
      }
     
       
       if(document.getElementsByClassName("status")[0].value  =="undefined"){
        document.getElementsByClassName("status")[0].value = ["belum kawin", "kawin", "cerai hidup", "cerai mati"][Math.floor(Math.random() * 4)]
      }
    
      
       if(document.getElementsByClassName("pek")[0].value =="undefined"){
         document.getElementsByClassName("pek")[0].value = "artis"
      }

        if(document.getElementsByClassName("kew")[0].value  =="undefined"){
           document.getElementsByClassName("kew")[0].value = "WIN"
         }
      
       if(document.getElementsByClassName("ber")[0].value  =="undefined"){
        document.getElementsByClassName("ber")[0].value = "SEUMUR HIDUP"
      }
    
      
      //- ------------------------------------------side container--------------------------//
       if(document.getElementsByClassName("region2")[0].value =="undefined"){
        document.getElementsByClassName("region2")[0].value = singleUserData.city1
      }
   
       
       if(document.getElementsByClassName("tem")[0].value =="undefined"){
       document.getElementsByClassName("tem")[0].value = singleUserData.date
      }
     